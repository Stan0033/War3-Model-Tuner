<Window x:Class="Wa3Tuner.edittags"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wa3Tuner"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="edit tags" Height="200" Width="200">
    <StackPanel Margin="10">
        <CheckBox Name="Check_b" Content="Billboarded"/>
        <CheckBox Name="Check_bx" Content="Billboarded Lock X"/>
        <CheckBox Name="Check_by" Content="Billboarded Lock Y"/>
        <CheckBox Name="Check_bz" Content="Billboarded Lock Z"/>
        <CheckBox Name="Check_a" Content="Camera Anchored"/>
        <CheckBox Name="Check_d1" Content="Dont inherit translation"/>
        <CheckBox Name="Check_d2" Content="Dont inherit rotation"/>
        <CheckBox Name="Check_d3" Content="Dont inherit scaling"/>
        <Button Click="ok" Content="OK"/>
    </StackPanel>
</Window>
