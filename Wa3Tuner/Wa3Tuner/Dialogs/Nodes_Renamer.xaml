<Window x:Class="Wa3Tuner.Dialogs.Nodes_Renamer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wa3Tuner.Dialogs"
        mc:Ignorable="d"
        KeyDown="Window_KeyDown"
        Title="Nodes_Renamer" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Write the new name next to the name of the node"/>
        <TextBox Background="LightGray" Name="input" Grid.Row="1" AcceptsReturn="True" TextWrapping="Wrap"/>
        <Button Content="OK" Click="ok" Grid.Row="2"/>

    </Grid>
</Window>
