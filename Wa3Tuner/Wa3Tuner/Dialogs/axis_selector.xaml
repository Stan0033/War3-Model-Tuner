<Window x:Class="Wa3Tuner.axis_selector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wa3Tuner"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        KeyDown="Window_KeyDown"
        WindowStartupLocation="CenterScreen"
        Title="Axis Selector" Height="150" Width="150">
    <StackPanel Margin="10">
        <CheckBox Name="Check_x">X</CheckBox>
        <CheckBox Name="Check_y">Y</CheckBox>
        <CheckBox Name="Check_z">Z</CheckBox>
        <Button Content="OK" Click="ok"/>
    </StackPanel>
</Window>
