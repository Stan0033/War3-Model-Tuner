<Window x:Class="Wa3Tuner.Dialogs.geoset_import_manager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wa3Tuner.Dialogs"
        mc:Ignorable="d"
        Background="LightGray"
        ResizeMode="NoResize"
        Closing="Window_Closing"
        WindowStartupLocation="CenterScreen"
        Title="Custom Geoset Import Manager" Height="300" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox Name="list" SelectionMode="Multiple"/>
        <StackPanel Grid.Column="1">
            <Button Click="delete" Content="Delete" Padding="10" Margin="10"/>
            <Button Click="rename" Content="Rename" Padding="10" Margin="10"/>
            <Button Click="Import" Content="Import" Padding="10" Margin="10"/>
            <Button Click="all" Content="Select all" Margin="3" Padding="2"/>
            <Button Click="none" Content="Select none" Margin="3" Padding="2"/>
            <Button Click="inverse" Content="Select inverse" Margin="3" Padding="2"/>
        </StackPanel>
    </Grid>
</Window>
