<Window x:Class="Wa3Tuner.Swap_Sequences_Selector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wa3Tuner"
        mc:Ignorable="d"
        KeyDown="Window_KeyDown"
        Title="Swap Sequences Selector" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="30"/>


        </Grid.RowDefinitions>
        <ListBox Name="list1" Background="LightGray"/>
        <ListBox Name="list2" Grid.Column="1"  Background="LightGray"/>
        <Button Click="ok" Grid.Row="1" Grid.ColumnSpan="2" Content="Swap" FontSize="16"/>

    </Grid>
</Window>
